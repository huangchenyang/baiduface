<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baidu.idl.main.facesdk.paymentlibrary"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="22"
        android:targetSdkVersion="29" />
    <!-- 往sdcard中写入数据的权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 在sdcard中创建/删除文件的权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <!-- 往sdcard中读取数据的权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application>
        <activity android:name="com.baidu.idl.main.facesdk.paymentlibrary.activity.payment.FaceRgbNirDepthPaymentActivity" />
        <activity android:name="com.baidu.idl.main.facesdk.paymentlibrary.activity.payment.FaceDepthPaymentActivity" />
        <activity android:name="com.baidu.idl.main.facesdk.paymentlibrary.activity.payment.FaceNIRPaymentActivity" />
        <activity android:name="com.baidu.idl.main.facesdk.paymentlibrary.activity.payment.FaceRGBPaymentActivity" />
        <activity android:name="com.baidu.idl.main.facesdk.paymentlibrary.activity.payment.FaceLimPaymentActivity" />
        <activity android:name="com.baidu.idl.main.facesdk.paymentlibrary.activity.payment.FaceDeptrumPaymentActivity" />
        <activity android:name="com.baidu.idl.main.facesdk.paymentlibrary.setting.PaymentSettingActivity" />
    </application>

</manifest>